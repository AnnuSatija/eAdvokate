                    android:padding="15dp"
                    android:layout_marginHorizontal="20dp">
                    <ImageView
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:src="@drawable/myconsultation"
                        android:layout_gravity="center"
